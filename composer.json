{
	"name" : "comcduarte/com-mvc-skeleton",
	"description" : "COM MVC Skeleton Application",
	"type" : "project",
	"license" : "BSD-3-Clause",
	"keywords" : [
		"laminas",
		"mvc",
		"skeleton",
		"framework"
	],
	"config" : {
		"allow-plugins" : {
			"laminas/*" : true
		}
	},
	"require" : {
		"php" : "^8.0",
		"laminas/laminas-component-installer" : "~3.3",
		"laminas/laminas-development-mode" : "3.10.0",
		"laminas/laminas-mvc" : "~3.6",
		"laminas/laminas-mvc-plugins" : "~1.2",
		"laminas/laminas-navigation" : "2.18.1",
		"laminas/laminas-db" : "2.18.0",
		"laminas/laminas-crypt" : "~3.10",
		"laminas/laminas-session" : "~2.16",
		"laminas/laminas-authentication" : "2.15.0",
		"laminas/laminas-form" : "3.9.0",
		"laminas/laminas-i18n" : "2.22.1",
		"laminas/laminas-permissions-acl" : "2.14.0",
		"laminas/laminas-permissions-rbac" : "3.4.0",
		"laminas/laminas-mail" : "2.22.0",
		"laminas/laminas-log" : "2.16.1",
		"comcduarte/laminas-box" : "dev-release/0.0.1"
	},
	"autoload" : {
		"psr-4" : {
			"Application\\" : "module/Application/src/",
			"Acl\\" : "module/Acl/src",
			"Components\\" : "module/Components/src",
			"Settings\\" : "module/Settings/src",
			"User\\" : "module/User/src",
			"Employee\\" : "module/Employee/src",
			"Timecard\\" : "module/Timecard/src",
			"Report\\" : "module/Report/src",
			"Annotation\\" : "module/Annotation/src",
			"Files\\" : "module/Files/src",
			"Fmla\\" : "module/Fmla/src",
			"Leave\\" : "module/Leave/src",
			"Help\\" : "module/Help/src",
			"Laminas\\Html\\" : "module/Html/src"
		}
	},
	"scripts" : {
		"cs-check" : "phpcs",
		"cs-fix" : "phpcbf",
		"development-disable" : "laminas-development-mode disable",
		"development-enable" : "laminas-development-mode enable",
		"development-status" : "laminas-development-mode status",
		"post-create-project-cmd" : [
			"@development-enable",
			"php bin/update-gitignore.php"
		],
		"serve" : "php -S 0.0.0.0:8080 -t public",
		"test" : "phpunit"
	},
	"require-dev" : {
		"laminas/laminas-developer-tools" : "~2.1"
	}
}